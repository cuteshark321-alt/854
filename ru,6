-- V4æ§åˆ¶æŒ‰éˆ•
v4ControlBtn.MouseButton1Click:Connect(function()
    print("ğŸ”˜ V4æ§åˆ¶æŒ‰éˆ•è¢«é»æ“Š")
    
    if not v4ControlModule then
        showNotification("ğŸ”„ æ­£åœ¨é‡æ–°åˆå§‹åŒ–V4æ§åˆ¶...", Color3.fromRGB(255, 165, 0), 1)
        v4ControlModule = setupV4Control()
        if not v4ControlModule then
            return
        end
    end
    
    isV4ControlOn = not isV4ControlOn
    
    if isV4ControlOn then
        if v4ControlModule.hasV4 then
            local success = v4ControlModule.start()
            if success then
                v4ControlBtn.BackgroundColor3 = Color3.fromRGB(60, 220, 60)
                v4ControlBtn.Text = "ğŸ”¥ V4æ§åˆ¶ [ON]"
                showNotification("ğŸ”¥ V4æ§åˆ¶å·²é–‹å•Ÿ\næ¯ç§’è‡ªå‹•è¦ºé†’", Color3.fromRGB(0, 180, 0), 2)
            else
                isV4ControlOn = false
                v4ControlBtn.Text = "ğŸ”¥ V4æ§åˆ¶ [éŒ¯èª¤]"
                task.wait(1.5)
                v4ControlBtn.BackgroundColor3 = Color3.fromRGB(255, 100, 0)
                v4ControlBtn.Text = "ğŸ”¥ V4æ§åˆ¶"
            end
        else
            showNotification("âŒ æ­¤éŠæˆ²ä¸æ”¯æ´V4\nç‹€æ…‹: " .. v4ControlModule.v4Status, Color3.fromRGB(255, 0, 0), 2.5)
            isV4ControlOn = false
            v4ControlBtn.Text = "ğŸ”¥ V4æ§åˆ¶ [ç„¡æ•ˆ]"
            task.wait(1.5)
            v4ControlBtn.BackgroundColor3 = Color3.fromRGB(255, 100, 0)
            v4ControlBtn.Text = "ğŸ”¥ V4æ§åˆ¶"
        end
    else
        if v4ControlModule and v4ControlModule.stop then
            v4ControlModule.stop()
        end
        v4ControlBtn.BackgroundColor3 = Color3.fromRGB(255, 100, 0)
        v4ControlBtn.Text = "ğŸ”¥ V4æ§åˆ¶ [OFF]"
        showNotification("ğŸ›‘ V4æ§åˆ¶å·²é—œé–‰", Color3.fromRGB(200, 0, 0), 1.5)
    end
end)

-- ESPé€è¦–æŒ‰éˆ•
espBtn.MouseButton1Click:Connect(function()
    print("ğŸ”˜ ESPé€è¦–æŒ‰éˆ•è¢«é»æ“Š")
    
    if not espModule then
        showNotification("ğŸ”„ æ­£åœ¨é‡æ–°åˆå§‹åŒ–ESP...", Color3.fromRGB(255, 165, 0), 1)
        espModule = setupESP()
        if not espModule then
            return
        end
    end
    
    isESPOn = not isESPOn
    
    if isESPOn then
        espBtn.BackgroundColor3 = Color3.fromRGB(60, 220, 60)
        espBtn.Text = "ğŸ‘ï¸ ESPé€è¦– [ON]"
        showNotification("ğŸ‘ï¸ ESPé€è¦–å·²é–‹å•Ÿ\nåŒ…å«: åå­—+è·é›¢+è¡€é‡\nå°„ç·šè¿½è¸ª", Color3.fromRGB(0, 180, 0), 2)
        
        if espModule and espModule.start then
            espModule.start()
        end
    else
        espBtn.BackgroundColor3 = Color3.fromRGB(150, 50, 200)
        espBtn.Text = "ğŸ‘ï¸ ESPé€è¦– [OFF]"
        showNotification("ğŸ›‘ ESPé€è¦–å·²é—œé–‰", Color3.fromRGB(200, 0, 0), 1.5)
        
        if espModule and espModule.stop then
            espModule.stop()
        end
    end
end)

-- é—œé–‰æŒ‰éˆ•
closeButton.MouseButton1Click:Connect(function()
    print("ğŸ”˜ é—œé–‰æŒ‰éˆ•è¢«é»æ“Š")
    
    -- åœæ­¢æ‰€æœ‰åŠŸèƒ½
    if isFastAttackOn and fastAttackModule and fastAttackModule.stop then
        fastAttackModule.stop()
    end
    
    if isV4ControlOn and v4ControlModule and v4ControlModule.stop then
        v4ControlModule.stop()
    end
    
    if isESPOn and espModule and espModule.stop then
        espModule.stop()
    end
    
    closeButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
    closeButton.Text = "é—œé–‰ä¸­..."
    
    showNotification("ğŸ›‘ æ­£åœ¨é—œé–‰è…³æœ¬...", Color3.fromRGB(220, 60, 60), 1)
    
    task.wait(0.8)
    
    if screenGui then
        screenGui:Destroy()
    end
    
    showNotification("âœ… è…³æœ¬å·²é—œé–‰", Color3.fromRGB(0, 180, 0), 1)
end)

-- ESCéµé—œé–‰ï¼ˆåƒ…é›»è…¦ï¼‰
