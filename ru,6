-- ç¢ºä¿æŒ‰éˆ•æ­£å¸¸å·¥ä½œçš„æœ€çµ‚æª¢æŸ¥
task.spawn(function()
    task.wait(1)
    
    local function testButtonClickable(button, name)
        if button and button:IsA("TextButton") then
            local connection = button.MouseButton1Click:Connect(function()
                print("âœ… " .. name .. " æŒ‰éˆ•é»æ“Šæ¸¬è©¦æˆåŠŸ")
                connection:Disconnect()
            end)
            
            task.spawn(function()
                task.wait(0.1)
                if connection.Connected then
                    connection:Disconnect()
                end
            end)
            
            return true
        end
        return false
    end
    
    local fastAttackOk = testButtonClickable(fastAttackBtn, "æ¥µé€Ÿæ”»æ“Š")
    local aimlockOk = testButtonClickable(aimlockBtn, "Ace Aimlock")
    local ggFlyOk = testButtonClickable(ggFlyBtn, "GGé£›è¡Œå™¨")
    local v4ControlOk = testButtonClickable(v4ControlBtn, "V4æ§åˆ¶")
    local espOk = testButtonClickable(espBtn, "ESPé€è¦–")
    local closeOk = testButtonClickable(closeButton, "é—œé–‰")
    local minimizeOk = testButtonClickable(minimizeButton, "æœ€å°åŒ–")
    local gearOk = testButtonClickable(gearButton, "é½’è¼ª")
    
    if fastAttackOk and aimlockOk and ggFlyOk and v4ControlOk and espOk and closeOk and minimizeOk and gearOk then
        print("âœ… æ‰€æœ‰æŒ‰éˆ•åŠŸèƒ½æ­£å¸¸")
    else
        print("âš ï¸ éƒ¨åˆ†æŒ‰éˆ•å¯èƒ½æœ‰å•é¡Œ")
    end
end)

print("ğŸš€ å°å‚‘å…¨èƒ½è…³æœ¬ v4.2 å®Œæˆï¼æ¥µé€Ÿæ”»æ“ŠUIå·²æ•´åˆåˆ°ä¸»ç•Œé¢ä¸­ã€‚")
