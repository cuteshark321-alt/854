-- æ¥µé€Ÿæ”»æ“ŠæŒ‰éˆ•
fastAttackBtn.MouseButton1Click:Connect(function()
    print("ğŸ”˜ æ¥µé€Ÿæ”»æ“ŠæŒ‰éˆ•è¢«é»æ“Š")
    
    if not fastAttackModule then
        showNotification("ğŸ”„ æ­£åœ¨é‡æ–°åˆå§‹åŒ–å¿«é€Ÿæ”»æ“Š...", Color3.fromRGB(255, 165, 0), 1)
        fastAttackModule = setupFastAttack()
        if not fastAttackModule then
            return
        end
    end
    
    isFastAttackOn = not isFastAttackOn
    
    if isFastAttackOn then
        fastAttackBtn.BackgroundColor3 = Color3.fromRGB(60, 220, 60)
        fastAttackBtn.Text = "âš¡ æ¥µé€Ÿæ”»æ“Š [ON]"
        showNotification("âš¡ æ¥µé€Ÿæ”»æ“Šå·²é–‹å•Ÿ\nè·é›¢: 10000å–®ä½\né–“éš”: 0.0000000001ç§’", Color3.fromRGB(0, 180, 0), 1.5)
        
        if fastAttackModule and fastAttackModule.start then
            fastAttackModule.start()
        end
    else
        fastAttackBtn.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
        fastAttackBtn.Text = "âš¡ æ¥µé€Ÿæ”»æ“Š [OFF]"
        showNotification("ğŸ›‘ æ¥µé€Ÿæ”»æ“Šå·²é—œé–‰", Color3.fromRGB(200, 0, 0), 1.5)
        
        if fastAttackModule and fastAttackModule.stop then
            fastAttackModule.stop()
        end
    end
end)

-- Ace Aimlock æŒ‰éˆ•
aimlockBtn.MouseButton1Click:Connect(function()
    print("ğŸ”˜ Ace Aimlock æŒ‰éˆ•è¢«é»æ“Š")
    
    aimlockBtn.BackgroundColor3 = Color3.fromRGB(60, 220, 60)
    aimlockBtn.Text = "ğŸ¯ åŠ è¼‰ä¸­..."
    
    local loaded = loadAceAimlock()
    
    if loaded then
        aimlockBtn.BackgroundColor3 = Color3.fromRGB(60, 220, 60)
        aimlockBtn.Text = "ğŸ¯ åŠ è¼‰æˆåŠŸ"
    else
        aimlockBtn.BackgroundColor3 = Color3.fromRGB(220, 60, 60)
        aimlockBtn.Text = "ğŸ¯ åŠ è¼‰å¤±æ•—"
        task.wait(1.5)
        aimlockBtn.BackgroundColor3 = Color3.fromRGB(220, 150, 50)
        aimlockBtn.Text = "ğŸ¯ Ace Aimlock"
    end
end)

