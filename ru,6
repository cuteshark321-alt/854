        s.Position=UDim2.new(0.468421042,0,0.491228074,0)
        s.Size=UDim2.new(0,44,0,28)
        s.Font=Enum.Font.SourceSans
        s.Text="1"
        s.TextColor3=Color3.fromRGB(255, 255, 255)
        s.TextScaled=true
        s.TextSize=14
        s.TextWrapped=true
        s.ClearTextOnFocus=false
        s.TextEditable=true
        s.PlaceholderText="1-150"
        
        -- é—œé–‰æŒ‰éˆ•æ”¹æˆç°¡å–®çš„å­—æ¯"X"
        c.Name="Close"
        c.Parent=f
        c.Font=Enum.Font.SourceSans
        c.Size=UDim2.new(0,45,0,28)
        c.Text="X"
        c.TextSize=18
        c.Position=UDim2.new(0,0,-1,27)
        c.TextColor3=Color3.fromRGB(255, 255, 255)
        
        h.Name="minimize"
        h.Parent=f
        h.Font=Enum.Font.SourceSans
        h.Size=UDim2.new(0,45,0,28)
        h.Text="éš±è—"
        h.TextSize=20
        h.Position=UDim2.new(0,44,-1,27)
        h.TextColor3=Color3.fromRGB(255, 255, 255)
        
        h2.Name="minimize2"
        h2.Parent=f
        h2.Font=Enum.Font.SourceSans
        h2.Size=UDim2.new(0,45,0,28)
        h2.Text="é¡¯ç¤º"
        h2.TextSize=20
        h2.Position=UDim2.new(0,44,-1,57)
        h2.Visible=false
        h2.TextColor3=Color3.fromRGB(255, 255, 255)
        
        -- æ”¶é›†æ‰€æœ‰UIå…ƒç´ 
        local uiElements = {u, d, o, p, n, nc, t, s, c, h, h2, f}
        
        -- å¤šç¨®æ¼¸è®Šé¡è‰²ï¼ˆå½©è™¹è‰²ç³» + æ›´å¤šæŸ”å’Œé¡è‰²ï¼‰
        local colorPalette = {
            -- å½©è™¹è‰²
            Color3.fromRGB(255, 0, 0),      -- ç´…
            Color3.fromRGB(255, 127, 0),    -- æ©™
            Color3.fromRGB(255, 255, 0),    -- é»ƒ
            Color3.fromRGB(0, 255, 0),      -- ç¶ 
            Color3.fromRGB(0, 255, 255),    -- é’
            Color3.fromRGB(0, 0, 255),      -- è—
            Color3.fromRGB(75, 0, 130),     -- é›
            Color3.fromRGB(148, 0, 211),    -- ç´«
            
            -- æ›´å¤šæŸ”å’Œé¡è‰²
            Color3.fromRGB(255, 105, 180),  -- ç²‰ç´…
            Color3.fromRGB(255, 182, 193),  -- æ·ºç²‰ç´…
            Color3.fromRGB(173, 216, 230),  -- æ·ºè—
            Color3.fromRGB(152, 251, 152),  -- æ·ºç¶ 
            Color3.fromRGB(255, 228, 196),  -- ç±³è‰²
            Color3.fromRGB(221, 160, 221),  -- æ·ºç´«
            Color3.fromRGB(255, 222, 173),  -- æ·ºæ©™
            Color3.fromRGB(175, 238, 238),  -- é’ç¶ è‰²
        }
        
        -- å¹³æ»‘é¡è‰²éæ¸¡å‡½æ•¸
        local function lerpColor(color1, color2, t)
            return Color3.new(
                color1.R + (color2.R - color1.R) * t,
                color1.G + (color2.G - color1.G) * t,
                color1.B + (color2.B - color1.B) * t
            )
        end
        
        -- é¡è‰²å‹•ç•«ç‹€æ…‹
        local currentColorIndex = 1
        local nextColorIndex = 2
        local transitionProgress = 0
        local transitionSpeed = 0.3 -- éæ¸¡é€Ÿåº¦ï¼ˆè¼ƒæ…¢æ›´è‡ªç„¶ï¼‰
        local currentColor = colorPalette[1]
        
        -- é¡è‰²å‹•ç•«æ›´æ–°
        local colorConnection
        colorConnection = R.Heartbeat:Connect(function(deltaTime)
            -- æ›´æ–°éæ¸¡é€²åº¦
            transitionProgress = transitionProgress + deltaTime * transitionSpeed
            
            if transitionProgress >= 1 then
                -- å®Œæˆéæ¸¡ï¼Œåˆ‡æ›åˆ°ä¸‹ä¸€å€‹é¡è‰²
                transitionProgress = 0
                currentColorIndex = nextColorIndex
                nextColorIndex = nextColorIndex + 1
                if nextColorIndex > #colorPalette then
                    nextColorIndex = 1
                end
            end
            
            -- è¨ˆç®—ç•¶å‰é¡è‰²
            currentColor = lerpColor(
                colorPalette[currentColorIndex],
                colorPalette[nextColorIndex],
                transitionProgress
            )
            
            -- æ‡‰ç”¨é¡è‰²åˆ°æ‰€æœ‰å…ƒç´ 
            for _, element in ipairs(uiElements) do
                element.BackgroundColor3 = currentColor
                
                -- è¨ˆç®—é‚Šæ¡†é¡è‰²ï¼ˆæ¯”èƒŒæ™¯ç¨äº®ï¼‰
                local borderColor = Color3.new(
                    math.min(currentColor.R * 1.3, 1),
                    math.min(currentColor.G * 1.3, 1),
                    math.min(currentColor.B * 1.3, 1)
                )
                element.BorderColor3 = borderColor
                element.BorderSizePixel = 2
                
                -- æ™ºèƒ½æ–‡å­—é¡è‰²ï¼šæ ¹æ“šäº®åº¦æ±ºå®šæ–‡å­—é¡è‰²
                local brightness = (currentColor.R * 0.299 + currentColor.G * 0.587 + currentColor.B * 0.114)
                if brightness > 0.5 then
                    element.TextColor3 = Color3.new(0, 0, 0)  -- æ·±è‰²èƒŒæ™¯ç”¨ç™½è‰²æ–‡å­—
                else
                    element.TextColor3 = Color3.new(1, 1, 1)  -- æ·ºè‰²èƒŒæ™¯ç”¨é»‘è‰²æ–‡å­—
                end
            end
        end)
        
        -- é£›è¡Œé‚è¼¯
        local F={}
        F.__index=F
        function F.new()
        local self=setmetatable({},F)
        self.v=1
        self.f=false
        self.u=false
        self.d=false
        self.n=false
        self.c={}
        self.l=nil
        self.w={}
        self.m=nil
        self.nc=nil
        self.nl=nil
        self.p={}
        self.a=nil
        self.pl=P.LocalPlayer
        self.ch=nil
        self.h=nil
        self.r=nil
        self.i=nil
        self:e()
        return self end
        function F:e()
        self:uC()
        local x=self.pl.CharacterAdded:Connect(function()
        task.wait(0.1)
        self:uC()
        self:sF()
        if self.ch then
        local a=self.ch:FindFirstChild("Animate")
        if a then a.Disabled=false end end
        if self.h then
        self.h.PlatformStand=false
        self.h:ResetState() end
        if self.n then
        task.wait(0.2)
        self:aN() end end)
        table.insert(self.c,x) end
        function F:uC()
        self.ch=self.pl.Character
        if self.ch then
        self.h=self.ch:FindFirstChildWhichIsA("Humanoid")
        self.r=self.ch:FindFirstChild("HumanoidRootPart") end end
        function F:cl()
        for _,x in ipairs(self.c) do if x then x:Disconnect() end end
        self.c={}
        self:sA()
        self:clP()
        self:sI()
        self:sN() end
        function F:sA()
        if self.l then self.l:Disconnect() self.l=nil end
        if self.m then self.m:Disconnect() self.m=nil end
        for _,x in ipairs(self.w) do if x then x:Disconnect() end end
        self.w={} end
        function F:clP()
        for _,x in ipairs(self.p) do if x and (x:IsA("BodyGyro") or x:IsA("BodyVelocity")) then x:Destroy() end end
        self.p={} end
        function F:pI()
        if not self.h then return end
        self:sI()
        local a=Instance.new("Animation")
        a.AnimationId="rbxassetid://507766666"
        self.i=self.h:LoadAnimation(a)
        if self.i then
        self.i:Play()
        self.i.Looped=true
        self.i.Priority=Enum.AnimationPriority.Idle end end
        function F:sI()
        if self.i then self.i:Stop() self.i=nil end end
        function F:aN()
        if not self.ch then return end
        self:sN()
        for _,x in ipairs(self.ch:GetDescendants()) do
        if x:IsA("BasePart") then
        x.CanCollide=false
        x.CollisionGroup="NoCollision" end end
        self.nc=self.ch.DescendantAdded:Connect(function(x)
        if x:IsA("BasePart") then
        x.CanCollide=false
        x.CollisionGroup="NoCollision" end end)
        self.nl=R.Heartbeat:Connect(function()
        if not self.ch or not self.n then return end
        for _,x in ipairs(self.ch:GetDescendants()) do
        if x:IsA("BasePart") then x.CanCollide=false end end end) end
        function F:rN()
        if not self.ch then return end
        if self.nc then self.nc:Disconnect() self.nc=nil end
        if self.nl then self.nl:Disconnect() self.nl=nil end
        for _,x in ipairs(self.ch:GetDescendants()) do
        if x:IsA("BasePart") then
        x.CanCollide=true
        x.CollisionGroup="Default" end end end
        function F:sN()
        if self.nc then self.nc:Disconnect() self.nc=nil end
        if self.nl then self.nl:Disconnect() self.nl=nil end end
        function F:dH()
        if not self.h then return end
        local s={
        Enum.HumanoidStateType.Climbing,
        Enum.HumanoidStateType.FallingDown,
        Enum.HumanoidStateType.Flying,
        Enum.HumanoidStateType.Freefall,
        Enum.HumanoidStateType.GettingUp,
        Enum.HumanoidStateType.Jumping,
        Enum.HumanoidStateType.Landed,
        Enum.HumanoidStateType.Physics,
        Enum.HumanoidStateType.PlatformStanding,
        Enum.HumanoidStateType.Ragdoll,
        Enum.HumanoidStateType.Running,
        Enum.HumanoidStateType.RunningNoPhysics,
        Enum.HumanoidStateType.Seated,
        Enum.HumanoidStateType.StrafingNoPhysics,
        Enum.HumanoidStateType.Swimming}
        for _,x in ipairs(s) do self.h:SetStateEnabled(x,false) end
        self.h:ChangeState(Enum.HumanoidStateType.Swimming) end
        function F:eH()
        if not self.h then return end
        local s={
        Enum.HumanoidStateType.Climbing,
        Enum.HumanoidStateType.FallingDown,
        Enum.HumanoidStateType.Flying,
        Enum.HumanoidStateType.Freefall,
        Enum.HumanoidStateType.GettingUp,
        Enum.HumanoidStateType.Jumping,
        Enum.HumanoidStateType.Landed,
        Enum.HumanoidStateType.Physics,
        Enum.HumanoidStateType.PlatformStanding,
        Enum.HumanoidStateType.Ragdoll,
        Enum.HumanoidStateType.Running,
        Enum.HumanoidStateType.RunningNoPhysics,
        Enum.HumanoidStateType.Seated,
        Enum.HumanoidStateType.StrafingNoPhysics,
        Enum.HumanoidStateType.Swimming}
        for _,x in ipairs(s) do self.h:SetStateEnabled(x,true) end
        self.h:ChangeState(Enum.HumanoidStateType.RunningNoPhysics) end
        function F:sW()
        self:sW2()
        for i=1,self.v do
        local x=R.Heartbeat:Connect(function()
        if not self.ch or not self.h then return end
        local m=self.h.MoveDirection
        if m.Magnitude>0 then self.ch:TranslateBy(m) end end)
        table.insert(self.w,x) end end
        function F:sW2()
        for _,x in ipairs(self.w) do if x then x:Disconnect() end end
        self.w={} end
        function F:sR6()
        if not self.ch then return end
        local t=self.ch:FindFirstChild("Torso")
        if not t then return end
        local g=t:FindFirstChildWhichIsA("BodyGyro")
        local v=t:FindFirstChildWhichIsA("BodyVelocity")
        if g then g:Destroy() end
        if v then v:Destroy() end
        local g=Instance.new("BodyGyro")
        g.P=9e4
        g.maxTorque=Vector3.new(9e9,9e9,9e9)
        g.cframe=t.CFrame
        g.Parent=t
        table.insert(self.p,g)
        local v=Instance.new("BodyVelocity")
        v.velocity=Vector3.new(0,0.1,0)
        v.maxForce=Vector3.new(9e9,9e9,9e9)
        v.Parent=t
        table.insert(self.p,v)
        if self.f then self.h.PlatformStand=true end
        local c={f=0,b=0,l=0,r=0}
        local l={f=0,b=0,l=0,r=0}
        local m=50
        local cs=0
        self.l=R.RenderStepped:Connect(function()
        if not self.f or not self.h or self.h.Health<=0 then self.l:Disconnect() return end
        local i=self:gI()
        c=i
        if c.l+c.r~=0 or c.f+c.b~=0 then
        cs=cs+0.5+(cs/m)
        if cs>m then cs=m end
        elseif not (c.l+c.r~=0 or c.f+c.b~=0) and cs~=0 then
        cs=cs-1
        if cs<0 then cs=0 end end
        if (c.l+c.r)~=0 or (c.f+c.b)~=0 then
        v.velocity=self:cV(c,cs)
        l={f=c.f,b=c.b,l=c.l,r=c.r}
        elseif (c.l+c.r)==0 and (c.f+c.b)==0 and cs~=0 then
        v.velocity=self:cV(l,cs)
        else v.velocity=Vector3.new(0,0,0) end
        g.cframe=workspace.CurrentCamera.CoordinateFrame*CFrame.Angles(-math.rad((c.f+c.b)*50*cs/m),0,0) end) end
        function F:sR15()
        if not self.ch then return end
        local t=self.ch:FindFirstChild("UpperTorso")
        if not t then return end
        local g=t:FindFirstChildWhichIsA("BodyGyro")
        local v=t:FindFirstChildWhichIsA("BodyVelocity")
        if g then g:Destroy() end
        if v then v:Destroy() end
        local g=Instance.new("BodyGyro")
        g.P=9e4
        g.maxTorque=Vector3.new(9e9,9e9,9e9)
        g.cframe=t.CFrame
        g.Parent=t
        table.insert(self.p,g)
        local v=Instance.new("BodyVelocity")
        v.velocity=Vector3.new(0,0.1,0)
        v.maxForce=Vector3.new(9e9,9e9,9e9)
        v.Parent=t
        table.insert(self.p,v)
        if self.f then self.h.PlatformStand=true end
        local c={f=0,b=0,l=0,r=0}
        local l={f=0,b=0,l=0,r=0}
        local m=50
        local cs=0
        self.l=R.Heartbeat:Connect(function()
        if not self.f or not self.h or self.h.Health<=0 then self.l:Disconnect() return end
        local i=self:gI()
        c=i
        if c.l+c.r~=0 or c.f+c.b~=0 then
        cs=cs+0.5+(cs/m)
        if cs>m then cs=m end
        elseif not (c.l+c.r~=0 or c.f+c.b~=0) and cs~=0 then
        cs=cs-1
        if cs<0 then cs=0 end end
        if (c.l+c.r)~=0 or (c.f+c.b)~=0 then
        v.velocity=self:cV(c,cs)
        l={f=c.f,b=c.b,l=c.l,r=c.r}
        elseif (c.l+c.r)==0 and (c.f+c.b)==0 and cs~=0 then
        v.velocity=self:cV(l,cs)
        else v.velocity=Vector3.new(0,0,0) end
        g.cframe=workspace.CurrentCamera.CoordinateFrame*CFrame.Angles(-math.rad((c.f+c.b)*50*cs/m),0,0) end) end
        function F:gI()
        local i={f=0,b=0,l=0,r=0}
        if U:IsKeyDown(Enum.KeyCode.W) then i.f=1 end
        if U:IsKeyDown(Enum.KeyCode.S) then i.b=-1 end
        if U:IsKeyDown(Enum.KeyCode.A) then i.l=-1 end
        if U:IsKeyDown(Enum.KeyCode.D) then i.r=1 end
        return i end
        function F:cV(c,sp)
        local ca=workspace.CurrentCamera
        if not ca then return Vector3.new() end
        return (ca.CFrame.lookVector*(c.f+c.b))+((ca.CFrame*CFrame.new(c.l+c.r,(c.f+c.b)*0.2,0).p)-ca.CFrame.p)*sp end
        function F:sF()
        self:uC()
        if not self.ch or not self.h then return end
        self.f=true
        o.Text="åœæ­¢"
        local a=self.ch:FindFirstChild("Animate")
        if a then a.Disabled=true end
        if self.h then
        for _,x in ipairs(self.h:GetPlayingAnimationTracks()) do x:AdjustSpeed(0) end end
        self:pI()
        self:dH()
        self:sW()
        if self.h.RigType==Enum.HumanoidRigType.R6 then self:sR6() else self:sR15() end end
        function F:eF()
        self.f=false
        o.Text="é–‹å§‹"
        self:sA()
        self:eH()
        self:sW2()
        self:sI()
        self:clP()
        if self.ch then
        local a=self.ch:FindFirstChild("Animate")
        if a then a.Disabled=false end end
        if self.h then self.h.PlatformStand=false end end
        function F:tF()
        if self.f then self:eF() else self:sF() end end

        -- ä¿®æ”¹ tN å‡½æ•¸ - æ·»åŠ å° aN çš„èª¿ç”¨
        function F:tN()
            self.n = not self.n
            if self.n then
                nc.Text = "æ­£å¸¸æ¨¡å¼"
                self:aN()  -- å•Ÿç”¨ç©¿ç‰†æ¨¡å¼
            else
                nc.Text = "ç©¿ç‰†æ¨¡å¼"
                self:rN()  -- ç¦ç”¨ç©¿ç‰†æ¨¡å¼
            end 
        end

        function F:sS()
        local i=s.Text
        if i=="" then s.Text=tostring(self.v) return end
        local ns=tonumber(i)
        if ns then
        if ns<1 then ns=1 elseif ns>150 then ns=150 end
        self.v=math.floor(ns)
        s.Text=tostring(self.v)
        if self.f then self:sW2() self:sW() end
        else s.Text=tostring(self.v) end end
        function F:iS()
        if self.v<150 then
        self.v=self.v+1
        s.Text=tostring(self.v)
        if self.f then self:sW2() self:sW() end
        else s.Text="æœ€å¤§" task.wait(0.5) s.Text=tostring(self.v) end end
        function F:dS()
        if self.v>1 then
        self.v=self.v-1
        s.Text=tostring(self.v)
        if self.f then self:sW2() self:sW() end
        else s.Text="æœ€å°" task.wait(1) s.Text=tostring(self.v) end end

        local fm=F.new()

        o.MouseButton1Click:Connect(function() fm:tF() end)
        nc.MouseButton1Click:Connect(function() fm:tN() end)
        s.FocusLost:Connect(function() fm:sS() end)

        local mt=nil
        local function sm(x)
        if mt then mt:Disconnect() end
        mt=R.Heartbeat:Connect(function()
        if not fm.r then return end
        fm.r.CFrame=fm.r.CFrame*CFrame.new(0,x,0) end) end
        local function em()
        if mt then mt:Disconnect() mt=nil end end

        u.MouseButton1Down:Connect(function() sm(1) end)
        u.MouseButton1Up:Connect(em)
        u.MouseLeave:Connect(em)
        d.MouseButton1Down:Connect(function() sm(-1) end)
        d.MouseButton1Up:Connect(em)
        d.MouseLeave:Connect(em)

        p.MouseButton1Click:Connect(function() fm:iS() end)
        n.MouseButton1Click:Connect(function() fm:dS() end)

        c.MouseButton1Click:Connect(function()
            -- é—œé–‰é¡è‰²å‹•ç•«
            if colorConnection then
                colorConnection:Disconnect()
                colorConnection = nil
            end
            
            if fm.f then fm:eF() end
            fm:cl()
            m:Destroy() 
        end)

        h.MouseButton1Click:Connect(function()
            u.Visible=false
            d.Visible=false
            o.Visible=false
            p.Visible=false
            s.Visible=false
            n.Visible=false
            nc.Visible=false
            h.Visible=false
            h2.Visible=true
            f.BackgroundTransparency=1
            
            -- é€™è£¡ä¹Ÿæ”¹æˆç°¡å–®çš„"X"
            c.Text = "X"
            c.Font = Enum.Font.SourceSans
            c.TextSize = 18
            c.Position=UDim2.new(0,0,-1,57)
            
            h2.Text = "é¡¯ç¤º"
        end)

        h2.MouseButton1Click:Connect(function()
            u.Visible=true
            d.Visible=true
            o.Visible=true
            p.Visible=true
            s.Visible=true
            n.Visible=true
            nc.Visible=true
            h.Visible=true
            h2.Visible=false
            f.BackgroundTransparency=0
            
            -- é€™è£¡ä¹Ÿæ”¹æˆç°¡å–®çš„"X"
            c.Text = "X"
            c.Font = Enum.Font.SourceSans
            c.TextSize = 18
            c.Position=UDim2.new(0,0,-1,27)
            
            h.Text = "éš±è—"
        end)
        
        return true, "GGé£›è¡Œå™¨åŠ è¼‰æˆåŠŸ"
    end)
    
    if success then
        local success2, message = errorMessage
        if success2 then
            showNotification("âœ… " .. message .. "\nğŸ•Šï¸ ç‰¹è‰²: RGBæ¼¸è®Šé¡è‰²\nç©¿ç‰†æ¨¡å¼\né€Ÿåº¦èª¿ç¯€", Color3.fromRGB(0, 180, 0), 2.5)
            print("ğŸ‰ " .. message)
            return true
        else
            showNotification("âš ï¸ " .. message, Color3.fromRGB(255, 100, 0), 2.5)
            print("âš ï¸ GGé£›è¡Œå™¨åŠ è¼‰å¤±æ•—: " .. message)
            return false
        end
    else
        showNotification("âŒ åŠ è¼‰å¤±æ•—: " .. (errorMessage or "æœªçŸ¥éŒ¯èª¤"), Color3.fromRGB(255, 0, 0), 2.5)
        print("âŒ GGé£›è¡Œå™¨åŠ è¼‰éŒ¯èª¤: " .. (errorMessage or "æœªçŸ¥éŒ¯èª¤"))
        return false
    end
end

-- ========================
-- å¿«é€Ÿæ”»æ“Šç³»çµ±ï¼ˆæ•´åˆUIç‰ˆæœ¬ï¼‰
-- ========================
print("âš¡ åˆå§‹åŒ–å¿«é€Ÿæ”»æ“Šç³»çµ±...")

local isFastAttackOn = false
local fastAttackModule = nil
local attackLoop = nil
local lastAttackTime = 0
local attackCount = 0

-- æ›´æ–°æ”»æ“Šç‹€æ…‹é¡¯ç¤º
local function updateAttackStatus(status, color)
    attackStatusLabel.Text = "âš¡ æ¥µé€Ÿæ”»æ“Š: " .. status
    attackStatusLabel.TextColor3 = color
end

-- æ›´æ–°æ”»æ“Šçµ±è¨ˆé¡¯ç¤º
local function updateAttackStats(targets)
    local targetsText = targets > 0 and " (" .. targets .. "ç›®æ¨™)" or ""
    attackStatsLabel.Text = "è·é›¢: 10000 | é–“éš”: 0.0000000001s" .. targetsText
end

-- å¿«é€Ÿæ”»æ“ŠåŠŸèƒ½
local function setupFastAttack()
    showNotification("âš¡ æ­£åœ¨åˆå§‹åŒ–å¿«é€Ÿæ”»æ“Š...", Color3.fromRGB(255, 165, 0), 1)
    
    local success, result = pcall(function()
        -- å…¨å±€é–‹é—œ
        _G.FastAttack = true

        if _G.FastAttack then
            local _ENV = (getgenv or getrenv or getfenv)()

            local function SafeWaitForChild(parent, childName)
                local success, result = pcall(function()
                    return parent:WaitForChild(childName)
                end)
                if not success or not result then
                    warn("æœªæ‰¾åˆ°: " .. childName)
                end
                return result
            end

            local function WaitChilds(path, ...)
                local last = path
                for _, child in {...} do
                    last = last:FindFirstChild(child) or SafeWaitForChild(last, child)
                    if not last then break end
                end
                return last
            end

            local CollectionService = game:GetService("CollectionService")
            local ReplicatedStorage = game:GetService("ReplicatedStorage")
            local TeleportService = game:GetService("TeleportService")
            local RunService = game:GetService("RunService")
            local Players = game:GetService("Players")
            local Player = Players.LocalPlayer

            if not Player then
                warn("æœªæ‰¾åˆ°æœ¬åœ°ç©å®¶.")
                return
            end

            local Remotes = SafeWaitForChild(ReplicatedStorage, "Remotes")
            if not Remotes then return end

            local Validator = SafeWaitForChild(Remotes, "Validator")
            local CommF = SafeWaitForChild(Remotes, "CommF_")
            local CommE = SafeWaitForChild(Remotes, "CommE")

            local ChestModels = SafeWaitForChild(workspace, "ChestModels")
            local WorldOrigin = SafeWaitForChild(workspace, "_WorldOrigin")
            local Characters = SafeWaitForChild(workspace, "Characters")
            local Enemies = SafeWaitForChild(workspace, "Enemies")
            local Map = SafeWaitForChild(workspace, "Map")

            local EnemySpawns = SafeWaitForChild(WorldOrigin, "EnemySpawns")
            local Locations = SafeWaitForChild(WorldOrigin, "Locations")

            local RenderStepped = RunService.RenderStepped
            local Heartbeat = RunService.Heartbeat
            local Stepped = RunService.Stepped

            local Modules = SafeWaitForChild(ReplicatedStorage, "Modules")
            local Net = SafeWaitForChild(Modules, "Net")

            local sethiddenproperty = sethiddenproperty or function(...) return ... end
            local setupvalue = setupvalue or (debug and debug.setupvalue)
            local getupvalue = getupvalue or (debug and debug.getupvalue)

            -- è¨­ç½®æ”»æ“Šé–“éš”ç‚º 0.0000000001
            local Settings = {
                AutoClick = true,
                ClickDelay = 0.0000000001  -- æ”»æ“Šé–“éš”è¨­ç½®ç‚º 0.0000000001
            }

            local FastAttack = {
                Distance = 10000,  -- è¨­ç½®ç‚º 10000
                attackMobs = true,
                attackPlayers = true,
                Equipped = nil
            }

            local RegisterAttack = SafeWaitForChild(Net, "RE/RegisterAttack")
            local RegisterHit = SafeWaitForChild(Net, "RE/RegisterHit")

            local function IsAlive(character)
                return character and character:FindFirstChild("Humanoid") and character.Humanoid.Health > 0
            end

            local function ProcessEnemies(OthersEnemies, Folder)
                local BasePart = nil
                local targetCount = 0
                for _, Enemy in Folder:GetChildren() do
                    local Head = Enemy:FindFirstChild("Head")
                    if Head and IsAlive(Enemy) and Player:DistanceFromCharacter(Head.Position) < FastAttack.Distance then
                        if Enemy ~= Player.Character then
                            table.insert(OthersEnemies, { Enemy, Head })
                            BasePart = Head
                            targetCount = targetCount + 1
                        end
                    end
                end
                return BasePart, targetCount
            end

            function FastAttack:Attack(BasePart, OthersEnemies, targetCount)
                if not BasePart or #OthersEnemies == 0 then return end
                
                if RegisterAttack then
                    RegisterAttack:FireServer(Settings.ClickDelay)
                end
                
                if RegisterHit then
                    RegisterHit:FireServer(BasePart, OthersEnemies)
                end
                
                -- æ›´æ–°UIç‹€æ…‹
                updateAttackStatus("æ”»æ“Šä¸­", Color3.fromRGB(100, 255, 100))
                updateAttackStats(targetCount)
            end

            function FastAttack:AttackNearest()
                local OthersEnemies = {}
                local totalTargets = 0
                local Part1, count1 = ProcessEnemies(OthersEnemies, Enemies)
                totalTargets = totalTargets + count1
                
                local Part2, count2 = ProcessEnemies(OthersEnemies, Characters)
                totalTargets = totalTargets + count2
                
                if #OthersEnemies > 0 then
                    self:Attack(Part1 or Part2, OthersEnemies, totalTargets)
                else
                    updateAttackStatus("ç¯„åœå…§ç„¡ç›®æ¨™", Color3.fromRGB(255, 255, 100))
                    updateAttackStats(0)
                    task.wait(Settings.ClickDelay)
                end
            end

            function FastAttack:BladeHits()
                local Equipped = IsAlive(Player.Character) and Player.Character:FindFirstChildOfClass("Tool")
                if Equipped and Equipped.ToolTip ~= "Gun" then
                    self:AttackNearest()
                else
                    updateAttackStatus("æœªè£å‚™è¿‘æˆ°æ­¦å™¨", Color3.fromRGB(255, 100, 100))
                    updateAttackStats(0)
                    task.wait(0.1)  -- ç¨å¾®ç­‰å¾…ä¸€ä¸‹å†æª¢æŸ¥
                end
            end

            -- è¨ˆç®—æ¯ç§’æ”»æ“Šæ¬¡æ•¸
            local attacksPerSecond = math.floor(1 / Settings.ClickDelay)
            
            return {
                start = function()
                    print("â–¶ï¸ é–‹å•Ÿæ¥µé€Ÿæ”»æ“Š")
                    updateAttackStatus("å·²å•Ÿå‹• (" .. attacksPerSecond .. "æ¬¡/ç§’)", Color3.fromRGB(100, 255, 100))
                    
                    if attackLoop then
                        attackLoop:Disconnect()
                    end
                    
                    attackLoop = Heartbeat:Connect(function()
                        local currentTime = tick()
                        if currentTime - lastAttackTime >= Settings.ClickDelay then
                            FastAttack:BladeHits()
                            attackCount = attackCount + 1
                            lastAttackTime = currentTime
                        end
                    end)
                    
                    return true
                end,
                stop = function()
                    print("â¹ï¸ åœæ­¢æ¥µé€Ÿæ”»æ“Š")
                    updateAttackStatus("å·²åœæ­¢", Color3.fromRGB(255, 100, 100))
                    updateAttackStats(0)
                    
                    if attackLoop then
                        attackLoop:Disconnect()
                        attackLoop = nil
                    end
                end,
                toggle = function()
                    if isFastAttackOn then
                        return "stop"
                    else
                        return "start"
                    end
                end,
                getStats = function()
                    return {
                        distance = FastAttack.Distance,
                        delay = Settings.ClickDelay,
                        attacksPerSecond = attacksPerSecond,
                        attackCount = attackCount
                    }
                end
            }
        end
        
        return nil
    end)
    
    if success and result then
        showNotification("âœ… å¿«é€Ÿæ”»æ“Šåˆå§‹åŒ–å®Œæˆ\nè·é›¢: 10000å–®ä½\né–“éš”: 0.0000000001ç§’", Color3.fromRGB(0, 180, 0), 1.5)
        return result
    else
        local errMsg = result or "æœªçŸ¥éŒ¯èª¤"
        showNotification("âš ï¸ å¿«é€Ÿæ”»æ“Šåˆå§‹åŒ–å¤±æ•—: " .. errMsg, Color3.fromRGB(255, 0, 0), 2)
        print("âŒ å¿«é€Ÿæ”»æ“Šåˆå§‹åŒ–å¤±æ•—: " .. errMsg)
        return nil
    end
end

-- åˆå§‹åŒ–å¿«é€Ÿæ”»æ“Š
local fastAttackModule = setupFastAttack()

-- ========================
-- V4æ§åˆ¶ç³»çµ±
-- ========================
print("ğŸ® å•Ÿå‹•V4æ§åˆ¶ç³»çµ±...")

local isV4ControlOn = false
local v4Task = nil

-- V4è¦ºé†’å‡½æ•¸
local function callV4Awakening()
    local success, result = pcall(function()
        local Backpack = player:FindFirstChild("Backpack")
        if not Backpack then return false, "ç„¡èƒŒåŒ…" end
        
        local Awakening = Backpack:FindFirstChild("Awakening")
        if not Awakening then return false, "ç„¡AwakeningæŠ€èƒ½" end
        
        local RemoteFunc = Awakening:FindFirstChild("RemoteFunction")
        if not RemoteFunc then return false, "ç„¡RemoteFunction" end
        
        return RemoteFunc:InvokeServer(true)
    end)
    
    return success, result
end

-- åˆå§‹åŒ–V4æ§åˆ¶
local function setupV4Control()
    showNotification("ğŸ”¥ æ­£åœ¨åˆå§‹åŒ–V4æ§åˆ¶...", Color3.fromRGB(255, 165, 0), 1)
    
    local success, result = pcall(function()
        local Backpack = player:FindFirstChild("Backpack")
        local hasV4 = false
        local v4Status = "æœªæª¢æ¸¬"
        
        if Backpack then
            local Awakening = Backpack:FindFirstChild("Awakening")
            if Awakening then
                hasV4 = true
                v4Status = "å·²æª¢æ¸¬åˆ°AwakeningæŠ€èƒ½"
            else
                v4Status = "æœªæ‰¾åˆ°AwakeningæŠ€èƒ½"
            end
        else
            v4Status = "èƒŒåŒ…ä¸å­˜åœ¨"
        end
        
        print("ğŸ® V4ç‹€æ…‹: " .. v4Status)
        
        return {
            hasV4 = hasV4,
            v4Status = v4Status,
            start = function()
                if not hasV4 then
                    showNotification("âŒ V4ä¸å¯ç”¨: " .. v4Status, Color3.fromRGB(255, 0, 0), 2)
                    return false
                end
                
                isV4ControlOn = true
                
                if v4Task then
                    task.cancel(v4Task)
                end
                
                v4Task = task.spawn(function()
                    while isV4ControlOn do
                        local success, result = callV4Awakening()
                        if success then
                            print("âœ… V4èª¿ç”¨æˆåŠŸ")
                        else
                            print("âŒ V4èª¿ç”¨å¤±æ•—:", result)
                        end
                        task.wait(1)
                    end
                end)
                
                return true
            end,
            stop = function()
                isV4ControlOn = false
                
                if v4Task then
                    task.cancel(v4Task)
                    v4Task = nil
                end
            end,
            test = function()
                if not hasV4 then
                    showNotification("âŒ V4ä¸å¯ç”¨: " .. v4Status, Color3.fromRGB(255, 0, 0), 2)
                    return false, "V4ä¸å¯ç”¨"
                end
                
                local success, result = callV4Awakening()
                if success then
                    showNotification("âœ… V4æ¸¬è©¦æˆåŠŸ", Color3.fromRGB(0, 180, 0), 2)
                    return true, "æˆåŠŸ"
                else
                    showNotification("âŒ V4æ¸¬è©¦å¤±æ•—: " .. (result or "æœªçŸ¥éŒ¯èª¤"), Color3.fromRGB(255, 0, 0), 2)
                    return false, result
                end
            end
        }
    end)
    
    if success and result then
        local v4Info = result
        showNotification("âœ… V4æ§åˆ¶åˆå§‹åŒ–å®Œæˆ\nç‹€æ…‹: " .. v4Info.v4Status, Color3.fromRGB(0, 180, 0), 2)
        print("ğŸ‰ V4æ§åˆ¶åˆå§‹åŒ–æˆåŠŸ: " .. v4Info.v4Status)
        return result
    else
        local errMsg = result or "æœªçŸ¥éŒ¯èª¤"
        showNotification("âš ï¸ V4æ§åˆ¶åˆå§‹åŒ–å¤±æ•—: " .. errMsg, Color3.fromRGB(255, 0, 0), 2)
        print("âŒ V4æ§åˆ¶åˆå§‹åŒ–å¤±æ•—: " .. errMsg)
        return nil
    end
end

-- åˆå§‹åŒ–V4æ§åˆ¶
local v4ControlModule = setupV4Control()

-- ========================
-- ESPé€è¦–ç³»çµ±
-- ========================
print("ğŸ‘ï¸ åˆå§‹åŒ–ESPé€è¦–ç³»çµ±...")

local isESPOn = false
local espModule = nil

-- ESPé€è¦–åŠŸèƒ½
local function setupESP()
    showNotification("ğŸ‘ï¸ æ­£åœ¨åˆå§‹åŒ–ESPé€è¦–...", Color3.fromRGB(255, 165, 0), 1)
    
    local success, result = pcall(function()
        -- è·å–å¿…è¦çš„æœåŠ¡
        local RunService = game:GetService("RunService")
        local Players = game:GetService("Players")
        local Workspace = game:GetService("Workspace")
        local Camera = Workspace.CurrentCamera
        local LocalPlayer = Players.LocalPlayer
        
        -- ESPé…ç½®
        local ESPConfig = {
            MainSwitch = false,      -- é€è§†æ€»å¼€å…³
            ShowNameDistance = true, -- æ˜¾ç¤ºç©å®¶åå­—+è·ç¦»
            ShowTracer = true,       -- æ˜¾ç¤ºå°„çº¿è¿½è¸ª
            ShowHealth = true        -- æ˜¾ç¤ºç©å®¶è¡€é‡
        }
        
        -- å­˜å‚¨ESPå…ƒç´ 
        local ESPElements = {}
        
        -- ä¸€é”®å¼€å¯æ‰€æœ‰åŠŸèƒ½
        local function EnableAllESP()
            ESPConfig.MainSwitch = true
            ESPConfig.ShowNameDistance = true
            ESPConfig.ShowTracer = true
            ESPConfig.ShowHealth = true
            UpdateAllESP()
            print("âœ… å·²å¼€å¯æ‰€æœ‰ESPåŠŸèƒ½")
        end
        
        -- ä¸€é”®å…³é—­æ‰€æœ‰åŠŸèƒ½
        local function DisableAllESP()
            ESPConfig.MainSwitch = false
            ESPConfig.ShowNameDistance = false
            ESPConfig.ShowTracer = false
            ESPConfig.ShowHealth = false
            UpdateAllESP()
            print("âŒ å·²å…³é—­æ‰€æœ‰ESPåŠŸèƒ½")
        end
        
        -- åˆ‡æ¢æ‰€æœ‰åŠŸèƒ½
        local function ToggleAllESP()
            if ESPConfig.MainSwitch then
                DisableAllESP()
            else
                EnableAllESP()
            end
        end
        
        -- æ¸…ç†å‡½æ•°
        local function CleanupPlayerESP(player)
            if not ESPElements[player.UserId] then return end
            
            if ESPElements[player.UserId].NameHealthESP then
                ESPElements[player.UserId].NameHealthESP:Destroy()
                ESPElements[player.UserId].NameHealthESP = nil
            end
            if ESPElements[player.UserId].NameHealthUpdateConn then
                ESPElements[player.UserId].NameHealthUpdateConn:Disconnect()
                ESPElements[player.UserId].NameHealthUpdateConn = nil
            end
            
            if ESPElements[player.UserId].Tracer then
                ESPElements[player.UserId].Tracer:Remove()
                ESPElements[player.UserId].Tracer = nil
            end
            if ESPElements[player.UserId].TracerConn then
                ESPElements[player.UserId].TracerConn:Disconnect()
                ESPElements[player.UserId].TracerConn = nil
            end
        end
        
        -- åˆ›å»ºåå­—+è·ç¦»+è¡€é‡ESP
        local function CreateNameDistanceHealthESP(player)
            if not player.Character or not player.Character:FindFirstChild("Head") then return end
            local head = player.Character.Head
            
            if ESPElements[player.UserId] and ESPElements[player.UserId].NameHealthESP then
                ESPElements[player.UserId].NameHealthESP:Destroy()
            end
            
            local billboardGui = Instance.new("BillboardGui")
            billboardGui.Name = "NameDistanceHealthESP"
            billboardGui.Adornee = head
            billboardGui.Size = UDim2.new(0, 150, 0, 60)
            billboardGui.StudsOffset = Vector3.new(0, 3.5, 0)
            billboardGui.AlwaysOnTop = true
            billboardGui.Parent = head
            
            local container = Instance.new("Frame")
            container.Size = UDim2.new(1, 0, 1, 0)
            container.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            container.BackgroundTransparency = 0.5
            container.Parent = billboardGui
            
            local corner = Instance.new("UICorner")
            corner.CornerRadius = UDim.new(0, 4)
            corner.Parent = container
            
            local textContainer = Instance.new("Frame")
            textContainer.Size = UDim2.new(1, -10, 1, -10)
            textContainer.Position = UDim2.new(0, 5, 0, 5)
            textContainer.BackgroundTransparency = 1
            textContainer.Parent = container
            
            local layout = Instance.new("UIListLayout")
            layout.Padding = UDim.new(0, 2)
            layout.HorizontalAlignment = Enum.HorizontalAlignment.Left
            layout.VerticalAlignment = Enum.VerticalAlignment.Top
            layout.Parent = textContainer
            
            -- åå­—æ ‡ç­¾
            local nameLabel = Instance.new("TextLabel")
            nameLabel.Name = "NameLabel"
            nameLabel.Size = UDim2.new(1, 0, 0, 20)
            nameLabel.BackgroundTransparency = 1
            nameLabel.Text = player.Name
            nameLabel.TextColor3 = Color3.new(1, 1, 1)
            nameLabel.Font = Enum.Font.GothamSemibold
            nameLabel.TextSize = 14
            nameLabel.TextXAlignment = Enum.TextXAlignment.Left
            nameLabel.Parent = textContainer
            
            -- è¡€é‡æ ‡ç­¾
            local healthLabel = Instance.new("TextLabel")
            healthLabel.Name = "HealthLabel"
            healthLabel.Size = UDim2.new(1, 0, 0, 16)
            healthLabel.BackgroundTransparency = 1
            healthLabel.Text = "è¡€é‡: 100/100"
            healthLabel.TextColor3 = Color3.new(0, 1, 0)
            healthLabel.Font = Enum.Font.Gotham
            healthLabel.TextSize = 12
            healthLabel.TextXAlignment = Enum.TextXAlignment.Left
            healthLabel.Parent = textContainer
            
            -- è·ç¦»æ ‡ç­¾
            local distanceLabel = Instance.new("TextLabel")
            distanceLabel.Name = "DistanceLabel"
            distanceLabel.Size = UDim2.new(1, 0, 0, 16)
            distanceLabel.BackgroundTransparency = 1
            distanceLabel.Text = "0.0ç±³"
            distanceLabel.TextColor3 = Color3.new(1, 0.5, 0)
            distanceLabel.Font = Enum.Font.Gotham
            distanceLabel.TextSize = 12
            distanceLabel.TextXAlignment = Enum.TextXAlignment.Left
            distanceLabel.Parent = textContainer
            
            -- æ›´æ–°è¿æ¥
            local updateConnection = RunService.RenderStepped:Connect(function()
                if not billboardGui.Parent or not LocalPlayer.Character or not player.Character then 
                    return 
                end
                
                -- æ›´æ–°è·ç¦»
                local localRoot = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                local targetRoot = player.Character:FindFirstChild("HumanoidRootPart")
                if localRoot and targetRoot then
                    local distance = (localRoot.Position - targetRoot.Position).Magnitude
                    distanceLabel.Text = string.format("%.1fç±³", distance)
                end
                
                -- æ›´æ–°è¡€é‡
                local humanoid = player.Character:FindFirstChild("Humanoid")
                if humanoid then
                    local health = math.floor(humanoid.Health)
                    local maxHealth = math.floor(humanoid.MaxHealth)
                    local healthPercent = health / maxHealth
                    
                    if healthPercent > 0.7 then
                        healthLabel.TextColor3 = Color3.new(0, 1, 0)
                    elseif healthPercent > 0.3 then
                        healthLabel.TextColor3 = Color3.new(1, 1, 0)
                    else
                        healthLabel.TextColor3 = Color3.new(1, 0, 0)
                    end
                    
                    healthLabel.Text = string.format("è¡€é‡: %d/%d", health, maxHealth)
                end
            end)
            
            ESPElements[player.UserId] = ESPElements[player.UserId] or {}
            ESPElements[player.UserId].NameHealthESP = billboardGui
            ESPElements[player.UserId].NameHealthUpdateConn = updateConnection
        end
        
        -- åˆ›å»ºå°„çº¿è¿½è¸ªESP
        local function CreateTracerESP(player)
            if ESPElements[player.UserId] and ESPElements[player.UserId].Tracer then
                ESPElements[player.UserId].Tracer:Remove()
            end
            
            local tracer = Drawing.new("Line")
            tracer.Color = Color3.new(1, 0, 0)
            tracer.Thickness = 2
            tracer.Transparency = 0.8
            tracer.Visible = false
            
            local renderConnection = RunService.RenderStepped:Connect(function()
                if not ESPConfig.MainSwitch or not ESPConfig.ShowTracer then
                    tracer.Visible = false
                    return
                end
                
                local localChar = LocalPlayer.Character
                local targetChar = player.Character
                if not localChar or not targetChar then
                    tracer.Visible = false
                    return
                end
                
                local targetRoot = targetChar:FindFirstChild("HumanoidRootPart")
                if targetRoot then
                    local screenPos, isVisible = Camera:WorldToViewportPoint(targetRoot.Position)
                    if isVisible then
                        tracer.Visible = true
                        tracer.From = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
                        tracer.To = Vector2.new(screenPos.X, screenPos.Y)
                    else
                        tracer.Visible = false
                    end
                else
                    tracer.Visible = false
                end
            end)
            
            ESPElements[player.UserId] = ESPElements[player.UserId] or {}
            ESPElements[player.UserId].Tracer = tracer
            ESPElements[player.UserId].TracerConn = renderConnection
        end
        
        -- æ›´æ–°ç©å®¶ESP
        local function UpdatePlayerESP(player)
            if player == LocalPlayer then return end
            
            if not player.Character then
                CleanupPlayerESP(player)
                return
            end
            
            if not player.Character:FindFirstChild("Head") or not player.Character:FindFirstChild("HumanoidRootPart") then
                CleanupPlayerESP(player)
                return
            end
            
            if ESPConfig.MainSwitch then
                -- åˆ›å»ºå°„çº¿è¿½è¸ª
                if ESPConfig.ShowTracer then
                    CreateTracerESP(player)
                else
                    if ESPElements[player.UserId] and ESPElements[player.UserId].Tracer then
                        ESPElements[player.UserId].Tracer:Remove()
                        ESPElements[player.UserId].Tracer = nil
                    end
                    if ESPElements[player.UserId] and ESPElements[player.UserId].TracerConn then
                        ESPElements[player.UserId].TracerConn:Disconnect()
                        ESPElements[player.UserId].TracerConn = nil
                    end
                end
                
                -- åˆ›å»ºåå­—+è·ç¦»+è¡€é‡ESP
                if ESPConfig.ShowNameDistance or ESPConfig.ShowHealth then
                    CreateNameDistanceHealthESP(player)
                else
                    if ESPElements[player.UserId] and ESPElements[player.UserId].NameHealthESP then
                        ESPElements[player.UserId].NameHealthESP:Destroy()
                        ESPElements[player.UserId].NameHealthESP = nil
                    end
                    if ESPElements[player.UserId] and ESPElements[player.UserId].NameHealthUpdateConn then
                        ESPElements[player.UserId].NameHealthUpdateConn:Disconnect()
                        ESPElements[player.UserId].NameHealthUpdateConn = nil
                    end
                end
            else
                CleanupPlayerESP(player)
            end
        end
        
        -- æ›´æ–°æ‰€æœ‰ESP
        function UpdateAllESP()
            for _, player in pairs(Players:GetPlayers()) do
                if player ~= LocalPlayer then
                    UpdatePlayerESP(player)
                end
            end
        end
        
        -- å¤„ç†ç©å®¶é‡ç”Ÿ
        local function HandlePlayerRespawn(player)
            local character = player.Character
            if not character then
                character = player.CharacterAdded:Wait()
            end
            
            local head = character:WaitForChild("Head", 5)
            local rootPart = character:WaitForChild("HumanoidRootPart", 5)
            
            if head and rootPart then
                task.wait(0.5)
                UpdatePlayerESP(player)
            end
        end
        
        -- ç©å®¶ç®¡ç†
        Players.PlayerAdded:Connect(function(player)
            if player == LocalPlayer then return end
            
            player.CharacterAdded:Connect(function()
                task.spawn(HandlePlayerRespawn, player)
            end)
            
            player.CharacterRemoving:Connect(function()
                CleanupPlayerESP(player)
            end)
            
            if player.Character then
                task.spawn(HandlePlayerRespawn, player)
            end
        end)
        
        -- å¤„ç†ç°æœ‰ç©å®¶
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then
                if player.Character then
                    task.spawn(HandlePlayerRespawn, player)
                end
                
                player.CharacterAdded:Connect(function()
                    task.spawn(HandlePlayerRespawn, player)
                end)
                
                player.CharacterRemoving:Connect(function()
                    CleanupPlayerESP(player)
                end)
            end
        end
        
        -- æœ¬åœ°ç©å®¶
        LocalPlayer.CharacterAdded:Connect(function()
            task.wait(1)
            UpdateAllESP()
        end)
        
        -- å®šæœŸæ›´æ–°
        task.spawn(function()
            while true do
                task.wait(2)
                if ESPConfig.MainSwitch then
                    UpdateAllESP()
                end
            end
        end)
        
        return {
            start = function()
                print("â–¶ï¸ é–‹å•ŸESPé€è¦–")
                EnableAllESP()
                return true
            end,
            stop = function()
                print("â¹ï¸ é—œé–‰ESPé€è¦–")
                DisableAllESP()
            end,
            toggle = function()
                ToggleAllESP()
            end,
            update = function()
                UpdateAllESP()
            end
        }
    end)
    
    if success and result then
        showNotification("âœ… ESPé€è¦–åˆå§‹åŒ–å®Œæˆ\nåŒ…å«: åå­—+è·é›¢+è¡€é‡\nå°„ç·šè¿½è¸ª", Color3.fromRGB(0, 180, 0), 1.5)
        return result
    else
        local errMsg = result or "æœªçŸ¥éŒ¯èª¤"
        showNotification("âš ï¸ ESPé€è¦–åˆå§‹åŒ–å¤±æ•—: " .. errMsg, Color3.fromRGB(255, 0, 0), 2)
        print("âŒ ESPé€è¦–åˆå§‹åŒ–å¤±æ•—: " .. errMsg)
        return nil
    end
end

-- åˆå§‹åŒ–ESP
local espModule = setupESP()

-- ========================
-- UIæœ€å°åŒ–/æœ€å¤§åŒ–åŠŸèƒ½
-- ========================
print("âš™ï¸ è¨­ç½®UIæœ€å°åŒ–åŠŸèƒ½...")

local isMinimized = false
local isGearDragging = false
local lastMainFramePosition = mainFrame.Position

-- æœ€å°åŒ–UIåˆ°é½’è¼ª
local function minimizeUI()
    print("âš™ï¸ æœ€å°åŒ–UIåˆ°é½’è¼ª...")
    
    lastMainFramePosition = mainFrame.Position
    
    local gearOffsetX = 92
    local gearOffsetY = 155
    
    gearButton.Position = UDim2.new(
        lastMainFramePosition.X.Scale,
        lastMainFramePosition.X.Offset + gearOffsetX,
        lastMainFramePosition.Y.Scale,
        lastMainFramePosition.Y.Offset + gearOffsetY
    )
    
    mainFrame.Visible = false
    gearButton.Visible = true
    isMinimized = true
    minimizeButton.Text = "â–¡"
    
    showNotification("âš™ï¸ UIå·²æœ€å°åŒ–\né»æ“Šé½’è¼ªåœ–æ¨™æ¢å¾©", Color3.fromRGB(100, 150, 200), 1.5)
    print("âœ… UIæœ€å°åŒ–å®Œæˆï¼Œé½’è¼ªä½ç½®: " .. tostring(gearButton.Position))
end

-- æœ€å¤§åŒ–UIå¾é½’è¼ª
local function maximizeUI()
    print("âš™ï¸ å¾é½’è¼ªæ¢å¾©UI...")
    
    local currentGearPosition = gearButton.Position
    local gearOffsetX = 92
    local gearOffsetY = 155
    
    local newMainFramePosition = UDim2.new(
        currentGearPosition.X.Scale,
        currentGearPosition.X.Offset - gearOffsetX,
        currentGearPosition.Y.Scale,
        currentGearPosition.Y.Offset - gearOffsetY
    )
    
    gearButton.Visible = false
    mainFrame.Position = newMainFramePosition
    mainFrame.Visible = true
    lastMainFramePosition = newMainFramePosition
    isMinimized = false
    minimizeButton.Text = "âˆ’"
    
    showNotification("âš™ï¸ UIå·²æ¢å¾©", Color3.fromRGB(100, 150, 200), 1)
    print("âœ… UIæ¢å¾©å®Œæˆï¼Œä¸»æ¡†æ¶ä½ç½®: " .. tostring(newMainFramePosition))
end

-- æœ€å°åŒ–æŒ‰éˆ•é»æ“Šäº‹ä»¶
minimizeButton.MouseButton1Click:Connect(function()
    if isMinimized then
        maximizeUI()
    else
        minimizeUI()
    end
end)

-- é½’è¼ªæŒ‰éˆ•é»æ“Šäº‹ä»¶
gearButton.MouseButton1Click:Connect(function()
    if isMinimized then
        maximizeUI()
    end
end)

-- é½’è¼ªæŒ‰éˆ•æ‹–å‹•åŠŸèƒ½
local gearDragStartPosition = Vector2.new(0, 0)
local gearStartPosition = UDim2.new(0, 0, 0, 0)

gearButton.MouseButton1Down:Connect(function()
    if isMinimized then
        isGearDragging = true
        gearDragStartPosition = inputService:GetMouseLocation()
        gearStartPosition = gearButton.Position
        gearButton.BackgroundColor3 = Color3.fromRGB(80, 120, 180)
    end
end)

inputService.InputChanged:Connect(function(input)
    if isGearDragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local currentMouse = inputService:GetMouseLocation()
        local delta = currentMouse - gearDragStartPosition
        
        gearButton.Position = UDim2.new(
            gearStartPosition.X.Scale,
            gearStartPosition.X.Offset + delta.X,
            gearStartPosition.Y.Scale,
            gearStartPosition.Y.Offset + delta.Y
        )
    end
end)

inputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        if isGearDragging then
            isGearDragging = false
            gearButton.BackgroundColor3 = Color3.fromRGB(40, 60, 90)
            print("âœ… é½’è¼ªä½ç½®æ›´æ–°: " .. tostring(gearButton.Position))
        end
    end
end)

-- æ‰‹æ©Ÿè§¸æ§æ‹–å‹•ï¼ˆé½’è¼ªï¼‰
if isMobile then
    local gearTouchDragging = false
    local gearTouchStartPosition = Vector2.new(0, 0)
    local gearTouchStartPositionUDim = UDim2.new(0, 0, 0, 0)
    
    gearButton.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Touch and isMinimized then
            gearTouchDragging = true
            gearTouchStartPosition = input.Position
            gearTouchStartPositionUDim = gearButton.Position
            gearButton.BackgroundColor3 = Color3.fromRGB(80, 120, 180)
        end
    end)
    
    inputService.InputChanged:Connect(function(input)
        if gearTouchDragging and input.UserInputType == Enum.UserInputType.Touch then
            local delta = input.Position - gearTouchStartPosition
            gearButton.Position = UDim2.new(
                gearTouchStartPositionUDim.X.Scale,
                gearTouchStartPositionUDim.X.Offset + delta.X,
                gearTouchStartPositionUDim.Y.Scale,
                gearTouchStartPositionUDim.Y.Offset + delta.Y
            )
        end
    end)
    
    inputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Touch then
            if gearTouchDragging then
                gearTouchDragging = false
                gearButton.BackgroundColor3 = Color3.fromRGB(40, 60, 90)
                print("âœ… é½’è¼ªä½ç½®æ›´æ–°: " .. tostring(gearButton.Position))
            end
        end
    end)
end

